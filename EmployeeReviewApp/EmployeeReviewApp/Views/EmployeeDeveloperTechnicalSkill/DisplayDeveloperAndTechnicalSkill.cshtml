@model IEnumerable<EmployeeReviewApp.ViewModels.DeveloperSkillDetail>
@{
    ViewBag.Title = "DisplayDeveloperAndTechnicalSkill";
}

<h2>Developer Ratings</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Designation)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SkillName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ScaleName)
        </th>
    </tr>
    @foreach (var devSkill in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => devSkill.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => devSkill.Designation)
            </td>
            <td>
                @Html.DisplayFor(modelItem => devSkill.SkillName)
            </td>
            <td>
                <label style="font-weight:normal" title="@devSkill.Description">@devSkill.ScaleName</label>
            </td>
        </tr>
    }
</table>



